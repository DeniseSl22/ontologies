        <rdfs:subClassOf rdf:resource="http://purl.bioontology.org/ontology/npo#NPO_1338"/>
        <obo:IAO_0000115>A toxicity that inheres in any substances capable of interfering with normal growth, differentiation, development or behaviour as a consequence of developmental insult. </obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.enanomapper.org/onto/ENM_0000051 -->

    <owl:Class rdf:about="http://purl.enanomapper.org/onto/ENM_0000051">
        <rdfs:label xml:lang="en">EC10</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002162"/>
