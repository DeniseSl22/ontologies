    <owl:Class rdf:about="http://purl.enanomapper.org/onto/ENM_0000055">
        <rdfs:label xml:lang="en">RD50</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.enanomapper.org/onto/ENM_0000018"/>
        <rdfs:subClassOf rdf:resource="http://www.bioassayontology.org/bao#BAO_0002162"/>
        <obo:IAO_0000115>The exposure concentration that produces a 50% respiratory rate decrease</obo:IAO_0000115>
    </owl:Class>
    


    <!-- http://purl.enanomapper.org/onto/ENM_0000056 -->

    <owl:Class rdf:about="http://purl.enanomapper.org/onto/ENM_0000056">
